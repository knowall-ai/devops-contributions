{
  "name": "contributions-widget",
  "private": true,
  "version": "3.0.0",
  "scripts": {
    "dev": "webpack serve --mode development --port 3000 --hot",
    "package-dev": "node ./node_modules/gulp/bin/gulp.js",
    "package-release": "node ./node_modules/gulp/bin/gulp.js --release",
    "format": "prettier --write \"scripts/**/*.{ts,tsx}\" \"styles/**/*.scss\"",
    "format:check": "prettier --check \"scripts/**/*.{ts,tsx}\" \"styles/**/*.scss\"",
    "lint": "eslint scripts --ext .ts,.tsx --ignore-pattern \"__tests__\"",
    "lint:fix": "eslint scripts --ext .ts,.tsx --fix --ignore-pattern \"__tests__\"",
    "typecheck": "tsc --noEmit",
    "ci": "bun run lint && bun run format:check"
  },
  "devDependencies": {
    "@types/applicationinsights-js": "^1.0.7",
    "@types/jquery": "3.3.38",
    "@types/jqueryui": "1.12.7",
    "@types/knockout": "^3.4.40",
    "@types/react": "18",
    "@types/react-dom": "18",
    "@types/requirejs": "^2.1.28",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "del": "^3.0.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.34.0",
    "gulp": "^4.0.0",
    "gulp-inline": "^0.1.3",
    "gulp-inline-source": "^4.0.0",
    "gulp-sass": "^5.1.0",
    "gulp-tslint": "^8.1.1",
    "gulp-webpack": "^1.5.0",
    "office-ui-fabric-react": "^4.51.1",
    "prettier": "^3.2.5",
    "promise-polyfill": "^8.1.0",
    "requirejs": "^2.3.2",
    "sass": "^1.71.0",
    "ts-loader": "9",
    "tslint": "^5.4.3",
    "typescript": "5",
    "vss-web-extension-sdk": "^5.141.0",
    "webpack": "5",
    "webpack-bundle-analyzer": "^3.5.2",
    "webpack-cli": "5",
    "webpack-dev-server": "^5.2.3"
  },
  "dependencies": {
    "@fluentui/react-components": "^9.72.11",
    "react": "18",
    "react-activity-calendar": "^3.1.1",
    "react-dom": "18"
  }
}
